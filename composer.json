{
    "name": "dnetix/asoban",
    "description": "Asobancaria plain text report parser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Calle",
            "email": "dnetix@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dnetix\\Asoban\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/var-dumper": "^4.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "testdox": "XDEBUG_MODE=coverage vendor/bin/phpunit --testdox --coverage-clover coverage.xml",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --stop-on-violation --using-cache=no"
    }
}
